#version 120

///////////////////////////////
// Attribute coming from the cpu

attribute vec4 vPosition;

///////////////////////////////
// Attributes for the fragment shader

varying vec2 fUv;

void main(void)
{
  fUv = (vPosition.xy + 1.0) / 2.0;

  gl_Position = vec4(vPosition.xy, 0.0, 1.0);
}
